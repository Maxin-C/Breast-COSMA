  "scoring_rules": {
    "DASH 受损程度/症状评分": {
      "formula": {
        "type": "AVERAGE_SCALE",
        "items": ["q1", "q2", "q3", "q4", "q5", "q6", "q7", "q8", "q9", "q10", "q11"],
        "item_operation": "val",
        "n_source": "count",
        "offset": -1,
        "scale": 25
      },
      "is_optional": false,
      "description": "Calculated for Part I and II. ''n'' is the number of completed responses. May not be calculated if more than 1 item is missing."
    },
    "DASH 工作能力评分": {
      "formula": {
        "type": "AVERAGE_SCALE",
        "items": ["q12", "q13", "q14", "q15"],
        "item_operation": "val",
        "n_source": 4,
        "offset": -1,
        "scale": 25
      },
      "is_optional": true,
      "description": "Optional module score. May not be calculated if any item is missing."
    },
    "DASH 运动/音乐评分": {
      "formula": {
        "type": "AVERAGE_SCALE",
        "items": ["q16", "q17", "q18", "q19"],
        "item_operation": "val",
        "n_source": 4,
        "offset": -1,
        "scale": 25
      },
      "is_optional": true,
      "description": "Optional module score. May not be calculated if any item is missing."
    },
    "生理状况": {
      "formula": {
        "type": "SUM_SCALE",
        "items": ["GP1", "GP2", "GP3", "GP4", "GP5", "GP6", "GP7"],
        "item_operation": "4-val",
        "n_source": "count",
        "scale": 7,
        "divide_by_n": true
      },
      "is_optional": false,
      "description": "Calculated for section physical_wellbeing. ''n'' is the number of completed responses. GP1, GP2, ..., GP7 are the id of questions."
    },
    "社会家庭状况": {
      "formula": {
        "type": "SUM_SCALE",
        "items": ["GS1", "GS2", "GS3", "GS4", "GS5", "GS6"],
        "item_operation": "val",
        "n_source": "count",
        "scale": 6,
        "divide_by_n": true
      },
      "is_optional": false,
      "description": "Calculated for section social_family_wellbeing. ''n'' is the number of completed responses. GS1, GS2, ..., GS6 are the id of questions."
    },
    "情感状况": {
      "formula": {
        "type": "COMPLEX_SUM_SCALE",
        "items": [
          {"id": "GE1", "op": "4-val"},
          {"id": "GE2", "op": "val"},
